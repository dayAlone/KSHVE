@import "nib"

blue = #2e5fa7

full-width()
	float left
	width 100%

transition()
	transition-property(all)
	transition-duration(.3s)

vignettes()
	position relative
	.content
		full-width()
		padding 35px
	> .after, > .before
		opacity 1
		position absolute
		content ""
		width 100%
		height 100%
		pointer-events none
		top 5px
		left 35px
		border-top 1px solid white
		border-bottom 1px solid white
		svg
			position absolute
			&:nth-child(1)
				top -51px
				left -1px
			&:nth-child(2)
				top -51px
				right -1px
			&:nth-child(3)
				bottom -51px
				left -1px
			&:nth-child(4)
				bottom -51px
				right -1px
			max-width 30px
			path
				fill white
	> .before
		top 35px
		left 5px
		border-top 0
		border-bottom 0
		border-left 1px solid white
		border-right 1px solid white

b(name=null, position=null, color=null, size=null)
	background color url(/layout/images/name) position
	if size
		background-size size

.fotorama__arr,.fotorama__fullscreen-icon,.fotorama__video-close,.fotorama__video-play
	background-image url(/layout/plugins/fotorama.png)
	@media (-webkit-min-device-pixel-ratio:1.5),(min-resolution:144dpi)
		background-image url(/layout/images/plugins/fotorama@2x.png)

